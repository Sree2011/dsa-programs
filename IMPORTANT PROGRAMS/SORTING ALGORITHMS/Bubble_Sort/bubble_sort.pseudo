
DECLARE n OF INTEGER
INPUT n
DECLARE arr : ARRAY[1:n] OF INTEGER
INPUT arr
FUNCTION bubble_sort(arr,n)
    FOR i = 0 to n-1 do
        FOR j = 0 to n-i-1 do
            IF arr[j] > arr[j + 1]
                arr[j] <-> arr[j+1]
            END IF
        END FOR
    END FOR
    RETURN arr
END FUNCTION